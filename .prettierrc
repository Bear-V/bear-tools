{
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "trailingComma": "none",
  "proseWrap": "always",
  "endOfLine": "auto",
  "arrowParens": "avoid",
  "importOrder": [
    ",^vite",
    ",^react",
    ",^antd",
    ",<THIRD_PARTY_MODULES>",
    "omponents/",
    "ages/",
    "ooks/",
    "tils/",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderCaseInsensitive": true
}

